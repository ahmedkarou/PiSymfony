{% extends 'test/index.html.twig' %}
 {% block clubbody %}
  <section class="schedule-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Our Schedule</h2>
                        <p>Do not miss anything topic about the event</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="schedule-tab">
                        
                        {% for club in clubs %}
            <div class="tab-pane active" id="tabs-{{ loop.index }}" role="tabpanel">
                <div class="st-content">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="sc-pic">
                                    <img src="{{ asset('uploads/image/' ~ club.image) }}" alt="">
                                </div>
                            </div>
                            <div class="col-lg-5">
                                <div class="sc-text">
                                    <h4>{{ club.Name }}</h4>
                                    <ul>
                                        <li><i class="fa fa-user"></i> {{ club.organizer}}</li>
                                        {# <li><i class="fa fa-envelope"></i> {{ club.getEmail() }}</li> #}
                                    </ul>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <ul class="sc-widget">
                                    {# <li><i class="fa fa-clock-o"></i> {{ club.getStartTime() }} - {{ club.getEndTime() }}</li> #}
                                    <li><i class="fa fa-map-marker"></i> {{ club.location }}</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </section>
 {%  endblock %}